{
    "name": "ng-template",
    "version": "0.0.0",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "lefthook install",
        "format": "pnpx @biomejs/biome format . --write --no-errors-on-unmatched --files-ignore-unknown=true",
        "lint": "pnpx @biomejs/biome lint . --apply --no-errors-on-unmatched --files-ignore-unknown=true",
        "check": "pnpx @biomejs/biome check . --diagnostic-level=error",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^20.3.3",
        "@angular/cdk": "^20.2.7",
        "@angular/common": "^20.3.3",
        "@angular/compiler": "^20.3.3",
        "@angular/core": "^20.3.3",
        "@angular/forms": "^20.3.3",
        "@angular/platform-browser": "^20.3.3",
        "@angular/platform-browser-dynamic": "^20.3.3",
        "@angular/router": "^20.3.3",
        "@auth0/angular-jwt": "5.0.2",
        "@ng-web-apis/common": "4.11.0",
        "@primeuix/themes": "^1.2.5",
        "@tailwindcss/postcss": "^4.1.14",
        "ngx-sonner": "^3.1.0",
        "postcss": "^8.5.6",
        "primeicons": "^7.0.0",
        "primeng": "^20.2.0",
        "rxjs": "~7.8.0",
        "tailwindcss": "^4.1.14",
        "tailwindcss-primeui": "^0.6.1",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@angular/build": "^20.3.4",
        "@angular/cli": "^20.3.4",
        "@angular/compiler-cli": "^20.3.3",
        "@biomejs/biome": "2.2.5",
        "@types/jasmine": "~5.1.0",
        "jasmine-core": "~5.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "lefthook": "^1.13.6",
        "postcss": "^8.5.3",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "typescript": "~5.9.3"
    },
    "optionalDependencies": {
        "@biomejs/cli-darwin-x64": "~2.2.3",
        "@biomejs/cli-linux-x64": "~2.2.3",
        "@biomejs/cli-win32-x64": "~2.2.3"
    },
    "packageManager": "pnpm@10.15.0",
    "engines": {
        "node": ">=22.x"
    }
}
